<ResourceDictionary
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:SolutionExtensions.ToolWindows"
             xmlns:model="clr-namespace:SolutionExtensions"
             xmlns:shell="clr-namespace:Microsoft.VisualStudio.Shell;assembly=Microsoft.VisualStudio.Shell.15.0"
             xmlns:ui="clr-namespace:Microsoft.VisualStudio.PlatformUI;assembly=Microsoft.VisualStudio.Shell.15.0"
             xmlns:cmt="uri:comments">
    <Style TargetType="ContextMenu" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ContextMenuStyleKey}}"/>
    <Style TargetType="TreeView" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ThemedDialogTreeViewStyleKey}}"/>
    <Style TargetType="ComboBox" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ThemedDialogComboBoxStyleKey}}"/>
    <Style TargetType="ListBox" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ThemedDialogListBoxStyleKey}}"/>
    <Style TargetType="Button" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ThemedDialogButtonStyleKey}}"/>
    <Style TargetType="Label" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.ThemedDialogLabelStyleKey}}"/>
    <Style TargetType="CheckBox" BasedOn="{StaticResource {x:Static shell:VsResourceKeys.CheckBoxStyleKey}}"/>

</ResourceDictionary>